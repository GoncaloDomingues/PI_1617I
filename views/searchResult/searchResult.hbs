
<div class="container">
    <h4>Foram obtidos {{artists.total}} resultados</h4>
    <div class="list-group">
        {{#each artists.items}}
        <div class="media">
            <div class ="media-left">
                <img class="media-object" src="data:image/svg+xml;base64," alt="..."
            </div>
        </div>
        <div class="media-body">
            <a href="/artists/{{{id}}}" class="list-group-item">{{name}}</a>
        </div>

        {{/each}}


            <div>

                {{#if first}}
                    <a class = "glyphicon glyphicon-step-backward" href = "/search/{{searchName}}"></a>
                {{/if}}
                {{#if left}}
                    <a class = "glyphicon glyphicon-chevron-left" href = "/search/{{searchName}}/{{previousPage}}"></a>
                {{/if}}

                {{#if right}}
                    <a class = "glyphicon glyphicon-chevron-right"  href = "/search/{{searchName}}/{{nextPage}}"></a>
                {{/if}}

                <a class = "glyphicon glyphicon-step-forward"  href = "/search/{{searchName}}/{{lastPage}}"></a>

            </div>

    </div>

</div> <!-- /container -->
